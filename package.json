{
  "name": "dis-edu",
  "version": "2.1.0",
  "main": "index.js",
  "description": "Databases course work template",
  "repository": "https://github.com/boldak/dis-edu",
  "author": "Andrey Boldak <boldak.andrey@gmail.com>",
  "license": "ECL 2.0",
  "scripts": {
    "start": "node ./src/app.js",
    "test": "jest",
    "migration": "npx sequelize-cli db:migrate --config src/configs/db.js",
    "docs:api": "jsdoc2md  src/**/*.js  > docs/api/README.md",
    "docs:dev": "npm run docs:api && npx vuepress dev docs",
    "docs:build": "npm run docs:api && npx vuepress build docs",
    "publish": "publish.sh"
  },
  "dependencies": {
    "@vuepress/plugin-back-to-top": "^1.7.1",
    "@vuepress/plugin-pwa": "^1.7.1",
    "bitcore-lib": "^8.25.8",
    "cors": "^2.8.5",
    "hdkey": "^2.0.1",
    "livr": "^2.3.3",
    "mysql2": "^2.1.0",
    "npx": "^10.2.2",
    "sequelize": "^6.19.0",
    "vuepress": "^1.7.1",
    "vuepress-theme-cool": "^1.3.1"
  },
  "devDependencies": {
    "jest": "^27.1.0",
    "jsdoc-to-markdown": "^7.0.1"
  },
  "jest": {
    "verbose": true,
    "globalTeardown": "./tests/jestGlobalTeardown.js",
    "globalSetup": "./tests/jestGlobalSetup.js",
    "collectCoverageFrom": [
      "**/lib/**/*.js",
      "**/bin/generateData/*.js",
      "!**/node_modules/**",
      "!**/lib/controllers/*.js",
      "!**/lib/router.js",
      "!**/lib/App.js",
      "!**/lib/errors/*.js"
    ]
  }
}
